Program Pzim ;
var 
i,j:Integer;

Begin
		i:=0;
	
    If 1<>1 then
    		writeln('andre')
		else 
				writeln('jogos');
				
		while i < 1 do
			begin
				writeln('while ',i);
				i:=i+1;
			end;
		
	  repeat
			i:=1+1;
			writeln('repeat ',i);
		until i = 2;
		
		for j:=1 to 1 do
		begin
				writeln('for ',j)
		end;
		
End.
